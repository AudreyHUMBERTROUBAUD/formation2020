    "x = linspace(0, 2*pi, 30)\n",
    "y1 = sin(x)\n",
    "y2 = sin(x + 3*pi/2)\n",
    "y3 = y1 + y2\n",
    "plot (x, y1, label = \"sin(x)\")\n",
    "plot (x, y2, label = \"sin(x + dephasage)\")\n",
    "plot (x, y3, label = \"sin(x) + sin(x + dephasage)\")\n",
    "legend()\n",
    "show()"
